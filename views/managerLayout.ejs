<!DOCTYPE html>
<html>
  <head>
    <% include partials/head.ejs %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script type="text/javascript">
      var sock = "";
      var index = 0; //vi tri bat dau lay thong bao
      var size = 10; //so thong bao
      var socket = io.connect("<%=conf%>");
      sock = socket;
    </script>
  </head>
  <body onload="loadRealTime('<%=conf%>','<%=token%>','<%=secu%>','<%=userid%>',sock);getNotification('<%=conf%>','<%=token%>','<%=secu%>','<%=userid%>',index,size);countNotification(socket,'<%=userid%>');"; class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
      <% include partials/nav.ejs %>
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Trang chủ
          </h1>
          <ol class="breadcrumb">
            <li><a href="/"><i class="fa fa-dashboard"></i>Trang chủ</a></li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <!-- Start main activity -->
          <span>Đây là trang chủ</span>
          <div class="modal fade" tabindex="-1" id="modalNotifi" role="dialog" aria-labelledby="gridSystemModalLabel">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                  <h4 class="modal-title" id="modal-title" id="gridSystemModalLabel"></h4>
                </div>
                <div class="modal-body">
                  <div class="row">
                    <div class="col-md-12">
                      <p class="DateCreated"></p>
                      <p class="RegionName"></p>
                      <p class="StationName"></p>
                      <p class="DataTypeName"></p>   
                      <p class="DataValue"></p>
                      <p class="ThresholdLevel"></p>
                      <p class="ThresholdMessage"></p>
                    </div>
                  </div>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
                </div>
              </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
          </div><!-- /.modal -->
        </section>
        <!-- /.content -->
      </div>
      <!-- /.content-wrapper -->
      <footer class="footer-title main-footer">
        <div class="pull-right hidden-xs">
          <b>Phiên bản</b> 1.0
        </div>
        <strong>Copyright &copy; Bộ môn Công Nghệ Phần Mềm,<a target="_blank" href="http://cit.ctu.edu.vn/"> Khoa CNTT & TT</a>, <a target="_blank" href="http://ctu.edu.vn/">Trường ĐH Cần Thơ.</a>.</strong>
      </footer>
      <% include partials/scripts.ejs %>
      <!-- Control Sidebar -->
      <% include partials/sidebar.ejs %>
    </div>
    <!-- ./wrapper -->
  </body>
  <!-- jQuery 2.2.3 -->
</html>
